{
  "name": "tailwind-documentation",
  "displayName": "Tailwind Documentation",
  "description": "Instant documentation search for Tailwind CSS",
  "version": "0.1.0",
  "icon": "src/tailwind-logo.png",
  "engines": {
    "vscode": "^1.45.1"
  },
  "keywords": [
    "tailwind",
    "documentation",
    "docs",
    "search",
    "shortcut"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alfredbirk/tailwind-documentation"
  },
  "publisher": "alfredbirk",
  "activationEvents": [
    "onCommand:tailwind-documentation.search"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tailwind-documentation.search",
        "title": "Tailwind Documentation Search"
      }
    ],
    "keybindings": [
      {
        "command": "tailwind-documentation.search",
        "key": "ctrl+alt+t",
        "mac": "ctrl+cmd+t"
      }
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@types/lodash": "^4.14.181",
    "@types/vscode": "^1.45.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "usehooks-ts": "^2.5.1"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "start": "react-scripts start",
    "build": "npm run compile",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "compile": "./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "pretest": "npm run compile"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.11",
    "@types/node-fetch": "^2.6.1",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "react-scripts": "^3.4.1",
    "rewire": "^5.0.0",
    "terser": "^4.7.0",
    "typescript": "^3.9.10"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}